        # s ▸ "ab"
        # for i in 0..itr
        #     s &= "c"
        # s.len ▸ itr+3 
        # profileStop "add2"
        # 1 ▸ 2
    
    ▸ num
    
        s = strg "a"
        s∙num() ▸ 1
        s += "bc"
        s∙num() ▸ 3
        
    ▸ index
    
        s = strg "abc"
        s[1] ▸ "a"
        s[2] ▸ "b"
        s[3] ▸ "c"
