    ▸ swapExt
    
        slash.swapExt "./some" "new"            ▸ "./some.new"
        slash.swapExt "./some.ext" "new"        ▸ "./some.new"
        slash.swapExt "./some.more.ext" "new"   ▸ "./some.more.new"
        
