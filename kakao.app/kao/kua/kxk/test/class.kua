class A
    a: 0
    @: a b ->
        @a = a
        @b = b
        
a = A(1 2)

class FunInc
    m: 0
    $: -> ⮐  "FunInc " & @m
    fun: m -> 
        @m = m
    inc: a1 ->
        @fun @m + a1
        log @
        
f = FunInc()

log f
f:inc 2
f:inc 2

