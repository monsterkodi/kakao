        slash.removeExt "./none"                ▸ "./none"
        slash.removeExt "./some.ext"            ▸ "./some"
        slash.removeExt "./some.more.ext"       ▸ "./some.more"
                                                
        slash.removeExt "../none"               ▸ "../none"
