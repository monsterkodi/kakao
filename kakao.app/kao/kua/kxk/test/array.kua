
use kxk/kxk

▸ array

    ▸ shift
    
        a = { 1 2 3 }
        x = array.shift a
        a ▸ { 2 3 }
        x ▸ 1
        x = array.shift a
        a ▸ { 3 }
        x ▸ 2
        x = array.shift a
        a ▸ {}
        x ▸ 3
        x = array.shift a
        a ▸ {}
        x ▸ nil
        
    ▸ isarr
    
        a = { 1 2 }
        array.isarr a ▸ true

        b = { c:1 d:2 }
        array.isarr b ▸ false
        
        c = {}
        array.isarr c ▸ false
        
        array.isarr false ▸ false
        
        array.isarr nil ▸ false
        
        d = {}
        d[2] = "d"
        array.isarr d ▸ true
        d[1] ▸ nil
        
        e = {}
        e.hello = "world"
        array.isarr e ▸ false
        e[1] = "and now?"
        array.isarr e ▸ true
        e.len ▸ 1
        e[1] = nil
        array.isarr e ▸ false
        e[3] = {}
        array.isarr e ▸ false
        e[2] = "{}"
        array.isarr e ▸ true
        e.len ▸ 3
        
    ▸ iter
    
        a = {}
        for i in iter(4 0) ➜ array.push a i
        a ▸ {4 3 2 1 0 }
        
        a = {}
        for i in iter(-4 4 2) ➜ array.push a i
        a ▸ {-4, -2, 0 2 4}
        
        a = {}
        for i in iter(4 0, -2) ➜ array.push a i
        a ▸ {4 2 0, -2, -4}
        
        a = {}
        for i in iter(4 2 2) ➜ array.push a i
        a ▸ {4 2 0, -2, -4}
        
        a = {}
        for i in iter(2/3, 1/3, 0.1) ➜ array.push a i
        a ▸ {2/3 2/3-0.1 2/3-0.2 2/3-0.3}
        
        a = {}
        for i in iter(2/3, 2/3, 1.001) ➜ array.push a i
        a ▸ {2/3}
        
        a = {}
        for i in iter(0.1+0.2, 0.3, 0.1) ➜ array.push a i
        a ▸ {0.3}
        
        a = {}
        for i in 0..3  ➜ array.push a i
        a ▸ {0 1 2 3}
        
        a = {}
        for i in 0...3 ➜ array.push a i
        a ▸ {0 1 2}
        
        a = {}
        for i in 0..<3 ➜ array.push a i
        a ▸ {0 1 2}
        
        a = {}
        for i in 3..0 ➜ array.push a i
        a ▸ {3 2 1 0}

        a = {}
        for i in 3...0 ➜ array.push a i
        a ▸ {}

        a = {}
        for i in 3..<0 ➜ array.push a i
        a ▸ {}
