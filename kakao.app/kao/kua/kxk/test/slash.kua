    ▸ home
        
        home = os.getenv "HOME"
            
        slash.home()                            ▸ home
        slash.home("sub" "dir")                 ▸ slash.path home "sub" "dir"
        slash.tilde home                        ▸ "~"
        slash.tilde home & "/sub"               ▸ "~/sub"
        slash.untilde "~/sub"                   ▸ home & "/sub"
        slash.untilde  "~/child"                ▸ slash.home() & "/child"

