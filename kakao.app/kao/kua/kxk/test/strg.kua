use kxk/kxk

▸ strg

    ▸ init
    
        $strg()         ▸ ""
        $strg("a")      ▸ "a"
        $strg("a" "b")  ▸ "ab"
        
    ▸ add
    
        itr = 100000
        profileStart "add"
        s = strg "a"
        s += "b"
        s∙num() ▸ 2
        $s ▸ "ab"
        for i in 0..itr
            s += "c"
        profileStop "add"
        profileStart "len"
        s∙len() ▸ itr+3
        profileStop "len"
        profileStart "num"
        s∙num() ▸ itr+3
        profileStop "num"
        
        # profileStart "add2"
        # s = "a"
        # s &= "b"
        # s.len ▸ 2
        # s ▸ "ab"
        # for i in 0..itr
        #     s &= "c"
        # s.len ▸ itr+3 
        # profileStop "add2"
        # 1 ▸ 2
    
    ▸ num
    
        s = strg "a"
        s∙num() ▸ 1
        s += "bc"
        s∙num() ▸ 3
        
    ▸ index
    
        s = strg "abc"
        s[1] ▸ "a"
        s[2] ▸ "b"
        s[3] ▸ "c"

    ▸ lines
    
        s = strg "1\n2\n3"
        l = s∙lines()
        $l[1] ▸ "1"
        $l[2] ▸ "2"
        $l[3] ▸ "3"
        l[1] += "a"
        $l[1] ▸ "1a"
        l[2] += "b"
        $l[2] ▸ "2b"
        