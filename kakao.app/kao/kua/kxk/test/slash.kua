    ▸ isRelative
        
        slash.isRelative "."                    ▸ true
        slash.isRelative ".."                   ▸ true
        slash.isRelative ".././bla../../fark"   ▸ true
        slash.isRelative "..\\blafark"          ▸ true
        slash.isRelative "a/b"                  ▸ true
        slash.isRelative "/a/b"                 ▸ false
        slash.isRelative "~"                    ▸ false

        slash.isAbsolute "."                    ▸ false
