
use kxk/kxk

▸ array

    ▸ shift
    
        a = { 1 2 3 }
        x = array.shift a
        a ▸ { 2 3 }
        x ▸ 1
        x = array.shift a
        a ▸ { 3 }
        x ▸ 2
        x = array.shift a
        a ▸ {}
        x ▸ 3
        x = array.shift a
        a ▸ {}
        x ▸ nil
        
    ▸ isarr
    
        a = { 1 2 }
        array.isarr a ▸ true

        b = { c:1 d:2 }
        array.isarr b ▸ false
        
        c = {}
        array.isarr c ▸ false
        
        array.isarr false ▸ false
        
        array.isarr nil ▸ false
        
