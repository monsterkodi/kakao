    â–¸ blockFillets
    
        text = """
            hello
                world
            """
        lines = text.split '\n'
        lineFillets = lines.map (line) -> kstr.fillet line
        
        blockFillets = kstr.blockFillets lineFillets
        blockFillets â–¸ [
            line:   0
            indent: 0
            fillet: [
                match:   'hello'
                index:   0
                length:  5
                word:    true
                ] 
            blocks: [
                line:  1
                indent: 4
                fillet: [
                    match:  'world'
                    index:  4
                    length:  5
                    word:  true
                    ]
                blocks: []
                ]
            ]
            
        â–¸ unfilletBlocks 
        
            r = kstr.unfilletBlocks blockFillets
            r â–¸ text + '\n'
            
