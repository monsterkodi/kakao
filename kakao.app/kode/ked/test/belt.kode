‚ñ∏ tool belt

    ‚ñ∏ cells
        
        l = belt.linesForText """
            012
            abc
            XYZ
            """
            
        cells = belt.cellsForLines l
        
        rect = belt.cellsInRect cells 0 0 1 1
        for c in rect
            c.cell.fg = [255 0 0]
            
        cells[0][0].fg ‚ñ∏ [255 0 0]
        cells[1][1].fg ‚ñ∏ [255 0 0]
        cells[2][2].fg ‚ñ∏ []
        
        belt.clampCellRect cells 0 0 1 1 ‚ñ∏ [0 0 1 1]
        belt.clampCellRect cells -1 -1 1 1 ‚ñ∏ [0 0 1 1]
        
        belt.cellsInRect(cells 0 0 1 1).map((n) -> n.cell.char) ‚ñ∏ ['0' '1' 'a' 'b']
        belt.cellsInRect(cells 1 1 1 1).map((n) -> n.cell.char) ‚ñ∏ ['b']
        belt.cellsInRect(cells 0 0 2 0).map((n) -> n.cell.char) ‚ñ∏ ['0' '1' '2']
        belt.cellsInRect(cells 1 2 2 2).map((n) -> n.cell.char) ‚ñ∏ ['Y' 'Z']
        
        belt.cellNeighborsAtPos(cells 0 0).map((n) -> n.cell.char) ‚ñ∏ ['0' '1' 'a' 'b']

